{
  "development": {
    "username": "your_postgres_username",
    "password": "your_postgres_password",
    "database": "your_database_name",
    "host": "localhost",
    "port": 5432,
    "dialect": "postgres"
  },
  "test": {
    "username": "your_postgres_username",
    "password": "your_postgres_password",
    "database": "your_test_database",
    "host": "localhost",
    "port": 5432,
    "dialect": "postgres"
  },
  "production": {
    "use_env_variable": "DATABASE_URL",
    "dialect": "postgres",
    "dialectOptions": {
      "ssl": {
        "require": true,
        "rejectUnauthorized": false
      }
    }
  }
}
